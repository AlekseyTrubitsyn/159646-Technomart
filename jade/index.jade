                article.catalogItem
                    .catalogItem__imageContainer
                        img(src="img/catalog/bosch-bfg-3000.png" width="" height="").catalogItem__image
                        a(href="#").greenButton.catalogItem__hover-buy Купить
                        a(href="#").catalogItem__hover-bookmark В закладки                        
                    p.catalogItem__text Перфоратор BOSCH BFG&nbsp;3000
