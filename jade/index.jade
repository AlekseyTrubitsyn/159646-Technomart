                    p.catalogItem__text Перфоратор BOSCH BFG&nbsp;2000
                    p.catalogItem__price 12500 Р.
            .popularBrands.container.clearfix
                .popularTitleBlock
                    h2.popularTitleBlock__title Популярные производители
                    a(href="#").redButton.popularTitleBlock__button Все производители
                article.catalogBrand
                    img(src="img/brands/bosch.png").catalogBrand__image
                article.catalogBrand
                    img(src="img/brands/makita.png").catalogBrand__image
                article.catalogBrand
